{
  "name": "emberx",
  "type": "module",
  "version": "1.0.4",
  "author": "Izel Nakri",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": ">= 12.0"
  },
  "scripts": {
    "lint": "eslint . --ext=js,ts --cache",
    "dev": "node_modules/.bin/webpack serve",
    "test": "node_modules/.bin/qunitx tests/unit/string --browser",
    "test:debug": "node_modules/.bin/qunitx tests/unit/string --browser --debug",
    "test:browsers": "node_modules/.bin/webpack && node_modules/.bin/qunitx tests --browser --debug",
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts:watch": "tsc --watch"
  },
  "dependencies": {
    "@glimmer/babel-preset": "2.0.0-beta.16",
    "@glimmer/component": "2.0.0-beta.16",
    "@glimmer/core": "2.0.0-beta.16",
    "@glimmer/interfaces": "^0.79.1",
    "@glimmer/modifier": "2.0.0-beta.16",
    "@glimmer/tracking": "2.0.0-beta.16",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.1",
    "route-recognizer": "^0.3.4",
    "router_js": "^7.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.37.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@glimmer/babel-preset"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 Edge versions",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "qunitx": "^0.1.7",
    "webpack-cli": "^4.7.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  }
}
