{
  "name": "emberx",
  "type": "module",
  "version": "1.0.4",
  "author": "Izel Nakri",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": ">= 12.0"
  },
  "scripts": {
    "lint": "eslint . --ext=js,ts --cache",
    "dev": "parcel index.html tests/tests.html --no-cache",
    "test": "node_modules/.bin/qunitx tests/unit/string --browser",
    "test:debug": "node_modules/.bin/qunitx tests/unit/string --browser --debug",
    "test:browsers": "parcel tests/index.html",
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "ts:watch": "tsc --watch"
  },
  "dependencies": {
    "@glimmer/babel-preset": "2.0.0-beta.16",
    "@glimmer/component": "2.0.0-beta.16",
    "@glimmer/core": "2.0.0-beta.16",
    "@glimmer/interfaces": "0.79.1",
    "@glimmer/modifier": "2.0.0-beta.16",
    "@glimmer/tracking": "2.0.0-beta.16",
    "bootstrap": "^5.0.1",
    "route-recognizer": "^0.3.4",
    "router_js": "^7.3.0"
  },
  "browserslist": {
    "production": [
      "last 2 Edge versions",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@glimmer/babel-preset",
      "@babel/preset-typescript",
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@parcel/transformer-typescript-tsc": "2.0.0-beta.2",
    "parcel": "2.0.0-beta.2",
    "qunitx": "^0.1.7"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  }
}
